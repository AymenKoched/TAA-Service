{
  "name": "taa-backend",
  "version": "0.0.0",
  "private": true,
  "main": "src/bundle.js",
  "_moduleAliases": {
    "@": "./src"
  },
  "scripts": {
    "clean": "rimraf ./dist/*",
    "build": "npm run clean && rollup -c",
    "start": "node dist/bundle.js",
    "dev": "nodemon --inspect='0.0.0.0:9001' src/server.js --exec babel-node",
    "eslint:lint": "./node_modules/.bin/eslint src",
    "test": "jest --runInBand --coverage --forceExit --detectOpenHandles --coverageThreshold='{\"global\":{\"branches\":80,\"functions\":80,\"lines\":80,\"statements\":80}}'",
    "precommit": "lint-staged --debug",
    "prepare": "husky",
    "format": "prettier --write 'src/**/*.js'",
    "lint": "eslint . --max-warnings=0 --ext .ts,.tsx,.js,.jsx"
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.4.5",
    "express": "^4.18.2",
    "express-session": "^1.17.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "morgan": "~1.9.1",
    "mysql2": "^2.1.0",
    "node-rdkafka": "^2.18.0",
    "sequelize": "^6.3.4"
  },
  "devDependencies": {
    "@babel/cli": "^7.24.8",
    "@babel/core": "^7.24.9",
    "@babel/node": "^7.2.2",
    "@babel/preset-env": "^7.24.8",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.2",
    "@rollup/plugin-json": "^4.0.2",
    "@types/jest": "^24.0.23",
    "babel-eslint": "^10.0.3",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^5.0.2",
    "babel-plugin-transform-export-default": "^7.0.0-alpha.20",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "eslint": "^6.7.2",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.7.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-prettier": "^3.1.1",
    "husky": "^9.0.11",
    "jest": "^29.7.0",
    "lint-staged": "^9.5.0",
    "nodemon": "^3.0.2",
    "rimraf": "^2.6.2",
    "rollup": "^1.9.0",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-builtins": "^2.0.0",
    "rollup-plugin-node-resolve": "^3.3.0",
    "sequelize-cli": "^6.3.0",
    "supertest": "^4.0.2"
  }
}
