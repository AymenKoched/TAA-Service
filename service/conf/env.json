{
  "environment": "{{ENVIRONMENT}}",
  "server": {
    "port": 7001
  },
  "database": {
    "type": "mysql",
    "host": "{{DB_HOST}}",
    "port": "{{DB_PORT:number}}",
    "database": "{{DB_NAME}}",
    "username": "{{DB_USERNAME}}",
    "password": "{{DB_PASSWORD}}",
    "synchronize": true,
    "logging": true,
    "migrationsRun": false
  },
  "jwt": {
    "publicKeyPath": "./conf/certs/dev.public_key.pem",
    "privateKeyPath": "./conf/certs/dev.private_key.pem",
    "signOptions": {
      "expiresIn": "1h",
      "algorithm": "RS256"
    }
  },
  "mailer": {
    "mailHost": "{{MAIL_HOST}}",
    "mailPort": "{{MAIL_PORT:number}}",
    "mailUser": "{{MAIL_USER}}",
    "mailPassword": "{{MAIL_PASSWORD}}",
    "mailSender": "{{MAIL_FROM_ADDRESS}}",
    "mailSenderName": "{{MAIL_SENDER_NAME}}"
  },
  "front": {
    "baseUrl": "{{FRONTEND_URL}}",
    "resetPasswordUri": "auth/create-new-password",
    "activateAccountUri": "auth/activate-account"
  },
  "cors": {
    "origin": ["http://localhost:5173", "https://taa.vayetek.com"],
    "methods": ["GET", "POST", "PUT", "DELETE", "OPTIONS"]
  }
}
